{
    "title": "Printer",
    "description": "A printing job belonging to a 3D printer.",
    "type": "object",
    "properties": {
        "job_id": { "type": "integer" },
        "data": {
            "status": {
                "enum": ["processing", "slicing", "printing", "completed", "paused", "errored"],
                "type": "string"
            },
            "started": { "type": "date-time" },
            "completed": { "type": "date-time" },
            "file": {
              "name": { "type": "string" },
              "origin": { "type": "string" },
              "size": { "type": "integer" },
              "date": { "type": "date-time" }
            },
            "estimatedPrintTime": { "type": "integer" },
            "filament": {
              "length": { "type": "string" },
              "volume": { "type": "string" }
            },
            "progress": {
                "completion": { "type": "string" },
                "filePos": { "type": "integer" },
                "printTime": { "type": "integer" },
                "printTimeLeft": { "type": "integer" }
            },
        }
    },
    "required": ["job_id", "data"]
}